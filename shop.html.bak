<div style="height:100%;width:100%;">

<div class="container">
  <div class="mySlides">
    
    <img src="wall-connector-gen3.jpg" style="width:100%">
  </div>

  <div class="mySlides">
    
    <img src="hero_cyberquad.jpg" style="width:100%">
  </div>

  <div class="mySlides">
    
    <img src="hero-chill.jpg" style="width:100%">
  </div>
    
  <a class="prev" onclick="plusSlides(-1)">❮</a>
  <a class="next" onclick="plusSlides(1)">❯</a>

  <div class="caption-container">
    <p id="caption"></p>
  </div> 
    
